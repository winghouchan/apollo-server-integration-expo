{
  "name": "@as-integrations/expo",
  "version": "0.0.0",
  "description": "An Apollo Server integration for use with Expo API Routes",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "lint": "npm run lint:shell && npm run lint:source",
    "lint:package": "publint",
    "lint:shell": "shellcheck $(git ls-files '*.sh')",
    "lint:source": "eslint && prettier . --check",
    "lint:source:fix": "eslint --fix && prettier . --write --ignore-unknown",
    "prepare": "husky",
    "test": "./scripts/test.sh",
    "test:coverage": "npm run test -- --coverage"
  },
  "devDependencies": {
    "@apollo/server": "^5.2.0",
    "@apollo/server-integration-testsuite": "^5.2.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@jest/globals": "^30.2.0",
    "@tsconfig/node-lts": "^24.0.0",
    "@tsconfig/recommended": "^1.0.13",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^25.0.2",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.5.0",
    "expo-server": "^1.0.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "publint": "^0.3.16",
    "ts-jest": "^29.4.6",
    "tsdown": "^0.18.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "peerDependencies": {
    "@apollo/server": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts"
}
